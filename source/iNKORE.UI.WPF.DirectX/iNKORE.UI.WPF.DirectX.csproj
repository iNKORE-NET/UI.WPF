	</ItemGroup>
	<ItemGroup>
		<Content Include="Effects\DeeperColor.fx" />
	</ItemGroup>
	<ItemGroup>
		<EmbeddedResource Include="Libraries\DirectShowLib-2005.dll" />
	</ItemGroup>
</Project>